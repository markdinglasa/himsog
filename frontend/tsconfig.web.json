{
  "include": [
    "src/env.d.ts",
    "src/*.html",
    "src/**/*",
  ],
  "compilerOptions": {
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "composite": true,
    "jsx": "react-jsx",
    "noUnusedLocals": false,
    "baseUrl": ".",
    "paths": {
      "types/*": [
        ".src/types/src/*"
      ],
    }
  }
}